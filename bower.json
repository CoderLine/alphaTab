    "SharpKit",
    "Source",
    "packages",
    "tools",
    ".gitattributes",
    ".gitignore",
    ".gitmodules",
    "AlphaTab.sln",
    "AlphaTab.targets",
    "LICENSE",
    "README.md",
    "TODO",
    "logo.png"
