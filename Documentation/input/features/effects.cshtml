<div id="alphaTabBrushes" data-file="@Context.GetLink("/assets/files/features/Brush.gp5")"></div>
<script type="text/javascript">
	$('#alphaTabBrushes').alphaTab();
</script>  

<h2 id="slides">Slides</h2>

<p>
	There are several types of slides alphaTab can render. 
</p>

<div id="alphaTabSlides" data-file="@Context.GetLink("/assets/files/features/Slides.gp5")"></div>
<script type="text/javascript">
	$('#alphaTabSlides').alphaTab();
</script>    

