<table class="table table-striped table-condensed type-table">
    <tbody>    
        <tr>
            <td><code>void Tex(string tex) <span class="code-badge">.net</span></td>
        </tr>   
        <tr>
            <td><code>void Tex(string tex, int[] tracks) <span class="code-badge">.net</span></td>
        </tr>   
        <tr>
            <td><code>function tex(tex) <span class="code-badge">JavaScript</span></td>
        </tr>       
        <tr>
            <td><code>alphaTab('tex', tex) <span class="code-badge">jQuery</span></td>
        </tr>       
        <tr>
            <td><code>function tex(tex, tracks) <span class="code-badge">JavaScript</span></td>
        </tr>       
        <tr>
            <td><code>alphaTab('tex', tex, tracks) <span class="code-badge">jQuery</span></td>
        </tr>       
    </tbody>
</table>

