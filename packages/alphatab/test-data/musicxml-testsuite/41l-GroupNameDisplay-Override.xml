<score-partwise version="4.0">
  <identification>
    <miscellaneous>
      <miscellaneous-field name="description">If &lt;group-name-display&gt; is
          given, it overrides &lt;group-name&gt; for display.

          The first two staves use only &lt;group-name&gt;, while the third and
          fourth one (with the same &lt;group-name&gt; value) override it with
          a custom text.

          In a similar vein, &lt;group-abbreviation&gt; can be overridden with
          &lt;group-abbreviation-display&gt;, shown in the second system.

          The multi-line entries are generated by using ‘xml:space="preserve"’
          as an attribute within a &lt;display-text&gt; child of the
          &lt;xxx-display&gt; element.</miscellaneous-field>
    </miscellaneous>
  </identification>
  <part-list>
    <part-group type="start">
      <group-name>Group Name</group-name>
      <group-abbreviation>G. N.</group-abbreviation>
      <group-symbol>bracket</group-symbol>
      <group-barline>no</group-barline>
    </part-group>
    <score-part id="P1">
      <part-name></part-name>
    </score-part>
    <score-part id="P2">
      <part-name></part-name>
    </score-part>
    <part-group type="stop"/>
    <part-group type="start">
      <group-name>Group Name</group-name>
      <group-name-display>
        <display-text xml:space="preserve">Group Name
Override</display-text>
      </group-name-display>
      <group-abbreviation>G. N.</group-abbreviation>
      <group-abbreviation-display>
        <display-text xml:space="preserve">G. N.
Overr.</display-text>
      </group-abbreviation-display>
      <group-symbol>bracket</group-symbol>
      <group-barline>no</group-barline>
    </part-group>
    <score-part id="P3">
      <part-name></part-name>
    </score-part>
    <score-part id="P4">
      <part-name></part-name>
    </score-part>
    <part-group type="stop"/>
  </part-list>
